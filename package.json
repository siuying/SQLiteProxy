  "main": "main.js",
  "bin": {
    "sqliteproxy": "./main.js"
  },
  "dependencies": {
    "better-sqlite3": "^5.0.1",
    "better-sqlite3-mozporter": "^1.0.0",
    "body-parser": "1.18.3",
